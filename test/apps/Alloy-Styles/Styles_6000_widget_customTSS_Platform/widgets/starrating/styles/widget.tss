"View":{
	layout:'vertical',
	top:20
}


".testlabel[if=Alloy.Globals.custom1 if=Alloy.Globals.custom2]":{
	text:'Widget style class customTSS - Check ALLOY.JS file => "custom1" and "custom2" are true.'
}

".testlabel[if=Alloy.Globals.custom1 if!=Alloy.Globals.custom2]":{

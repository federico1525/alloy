}

".testlabel[if!=Alloy.Globals.custom1 if=Alloy.Globals.custom2]":{
	text:'Widget style class customTSS - Check ALLOY.JS file => "custom1" is false. "custom2" is true.'
}

".testlabel[if!=Alloy.Globals.custom1 if!=Alloy.Globals.custom2]":{
	text:'Widget style class customTSS - Check ALLOY.JS file => "custom1" and "custom2" are false.'
}